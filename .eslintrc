{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest-dom/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["jest", "jest-dom"],
  "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "single"],
      "comma-dangle": ["error", "never"]
  }
}
